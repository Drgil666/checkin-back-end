<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.SignMapper">
    <select id="getCheckSetBySign" parameterType="java.util.List" resultType="com.example.demo.pojo.CheckSet">
        select * from checkset where id in
        <foreach collection="id" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>
</mapper>